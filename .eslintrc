{
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "react-important-stuff",
    "standard"
  ],
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "always"],
    "semi-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "no-self-compare": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
